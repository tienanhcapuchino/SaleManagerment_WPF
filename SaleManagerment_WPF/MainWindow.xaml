<Window x:Class="SaleManagerment_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaleManagerment_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dtbMembers" d:ItemsSource="{d:SampleData ItemCount=5}" SelectionChanged="dtbMembers_SelectionChanged" Loaded="LoadData" Margin="0,0,300,241"/>
        <Label x:Name="lbUserName" Content="Label" HorizontalAlignment="Left" Margin="174,265,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbPassword" Content="Label" HorizontalAlignment="Left" Margin="351,278,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
